{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.3.*",
        "zendframework/zend-developer-tools": "1.0.0-BETA3@BETA",
        "ocramius/ocra-service-manager": "2.1.*",
        "spoonx/sxbootstrap": "2.*",
        "rwoverdijk/assetmanager": "1.3.*",
        "doctrine/annotations": "1.*",
        "zf-commons/zfc-user": "~1.0.0",
        "bjyoungblood/bjy-authorize": "~1.4.0",
        "zf-commons/zfc-rbac": "~2.3.0",
        "doctrine/doctrine-orm-module": "~0.8.0",
        "fzaninotto/faker": "~1.3.0",
        "doctrine/data-fixtures": "~1.0.0"
    },
    "autoload": {
        "psr-0": {
            "Blog\\":        "module/Blog/src",
            "Application\\": "module/Application/src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload-dev": {
        "psr-0": {
            "BlogTest\\":      "module/Blog/test",
            "BlogTestAsset\\": "module/Blog/test"
        }
    }
}
